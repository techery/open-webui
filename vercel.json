{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "dest": "backend"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "dest": "dist"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    }
  ]
}